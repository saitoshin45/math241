\begin{document}
\textbf{Directions:} show all your work and simplify your answers 
\begin{enumerate}
    \item \textbf{Graph the the following functions by hand and state the domains and the range.}
    \begin{itemize}
        \item [a)] $ f(x) = - x^{2}$
        \vspace{4cm}
        \item[b)] $f(x) = 2\sqrt{x+1}$
        \vspace{4cm}
    \end{itemize}
    \item \textbf{Find the functions $f \circ g, g\circ f, f \circ f, $ and $ g \circ g$} 
    \begin{itemize}
        \item [a)] $f(x) = x^{4}-3$ and $g(x) 1-6x$ \vspace{4cm}
        \item [b)] $f(x) = 5x-4$ and $g(x) = cos(x)$ 
        \vspace{4cm}
    \end{itemize}
\end{enumerate}
